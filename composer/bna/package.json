{
  "name": "digital-cmr-network",
  "version": "0.0.10",
  "description": "The Hello World of Hyperledger Composer samples",
  "scripts": {
    "prepublish": "mkdirp ./dist && composer archive create --sourceType dir --sourceName . -a ./dist/digital-cmr-network.bna",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.conf",
    "test-inner": "mocha --timeout 15000 --recursive && cucumber-js",
    "test-cover": "nyc npm run test-inner",
    "test": "npm run test-inner"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/composer-sample-networks.git"
  },
  "keywords": [
    "sample",
    "network"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "devDependencies": {
    "browserfs": "^1.4.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "composer-admin": "0.10.0",
    "composer-cli": "0.10.0",
    "composer-client": "0.10.0",
    "composer-connector-embedded": "0.10.0",
    "composer-cucumber-steps": "0.10.0",
    "cucumber": "^2.2.0",
    "eslint": "^3.6.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.3",
    "license-check": "^1.1.5",
    "mkdirp": "^0.5.1",
    "mocha": "^3.5.0",
    "moment": "^2.17.1",
    "nyc": "^11.1.0"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*",
      "!./scripts/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "features/**",
      "out/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  },
  "dependencies": {
    "passport-github": "^1.1.0"
  }
}
